%section
	.container
		.row
			.col-sm-9
				.panel
					.panel-body
						.page-header.no-margin-top
							%h4.no-margin-bottom
								Recent Activity
						-@check_ins.each do |c|
							=render partial: 'check_ins/post', locals:{:check_in => c}
							%hr
			.col-sm-3
				.panel
					.panel-body
						.page-header.no-margin-top
							%h5.no-margin-bottom
								Recently Added
						-@recent_liquors.each do |l|
							=render partial: 'liquors/liquor_thumb_small', locals:{:liquor => l}
